<div class="page-header">
  <%= link_to order_billings_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_order_billing_path(@order_billing), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show order_billing</h1>
</div>

<dl class="dl-horizontal">
  <dt>Order billingscol:</dt>
  <dd><%= @order_billing.order_billingscol %></dd>

  <dt>Billing amount:</dt>
  <dd><%= @order_billing.billing_amount %></dd>

  <dt>Billing method:</dt>
  <dd><%= @order_billing.billing_method %></dd>

  <dt>Billing status:</dt>
  <dd><%= @order_billing.billing_status %></dd>

  <dt>Discount:</dt>
  <dd><%= @order_billing.discount %></dd>

  <dt>Tax:</dt>
  <dd><%= @order_billing.tax %></dd>

  <dt>Note:</dt>
  <dd><%= @order_billing.note %></dd>

</dl>
