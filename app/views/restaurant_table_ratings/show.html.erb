<div class="page-header">
  <%= link_to restaurant_table_ratings_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_restaurant_table_rating_path(@restaurant_table_rating), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show restaurant_table_rating</h1>
</div>

<dl class="dl-horizontal">
  <dt>From user:</dt>
  <dd><%= @restaurant_table_rating.from_user_id %></dd>

  <dt>Review:</dt>
  <dd><%= @restaurant_table_rating.review %></dd>

  <dt>Table:</dt>
  <dd><%= @restaurant_table_rating.table_id %></dd>

  <dt>Rating:</dt>
  <dd><%= @restaurant_table_rating.rating %></dd>

  <dt>Status:</dt>
  <dd><%= @restaurant_table_rating.status %></dd>

  <dt>Creator:</dt>
  <dd><%= @restaurant_table_rating.creator_id %></dd>

  <dt>Modifier:</dt>
  <dd><%= @restaurant_table_rating.modifier_id %></dd>

  <dt>Create time:</dt>
  <dd><%= @restaurant_table_rating.create_time %></dd>

  <dt>Modified time:</dt>
  <dd><%= @restaurant_table_rating.modified_time %></dd>

</dl>
