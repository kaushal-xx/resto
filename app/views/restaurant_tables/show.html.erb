<div class="page-header">
  <%= link_to restaurant_tables_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_restaurant_table_path(@restaurant_table), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show restaurant_table</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @restaurant_table.name %></dd>

  <dt>Description:</dt>
  <dd><%= @restaurant_table.description %></dd>

  <dt>Max sitting:</dt>
  <dd><%= @restaurant_table.max_sitting %></dd>

  <dt>Restaurant:</dt>
  <dd><%= @restaurant_table.restaurant_id %></dd>

  <dt>Booking status:</dt>
  <dd><%= @restaurant_table.booking_status %></dd>

  <dt>Booking:</dt>
  <dd><%= @restaurant_table.booking_id %></dd>

  <dt>Status:</dt>
  <dd><%= @restaurant_table.status %></dd>

  <dt>Creator:</dt>
  <dd><%= @restaurant_table.creator_id %></dd>

  <dt>Modifier:</dt>
  <dd><%= @restaurant_table.modifier_id %></dd>

  <dt>Create time:</dt>
  <dd><%= @restaurant_table.create_time %></dd>

  <dt>Modified time:</dt>
  <dd><%= @restaurant_table.modified_time %></dd>

</dl>
