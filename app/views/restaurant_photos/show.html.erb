<div class="page-header">
  <%= link_to restaurant_photos_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_restaurant_photo_path(@restaurant_photo), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show restaurant_photo</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @restaurant_photo.name %></dd>

  <dt>Description:</dt>
  <dd><%= @restaurant_photo.description %></dd>

  <dt>Url:</dt>
  <dd><%= @restaurant_photo.url %></dd>

  <dt>Restaurant:</dt>
  <dd><%= @restaurant_photo.restaurant_id %></dd>

  <dt>Photo type:</dt>
  <dd><%= @restaurant_photo.photo_type %></dd>

  <dt>Status:</dt>
  <dd><%= @restaurant_photo.status %></dd>

  <dt>Creator:</dt>
  <dd><%= @restaurant_photo.creator_id %></dd>

  <dt>Modifier:</dt>
  <dd><%= @restaurant_photo.modifier_id %></dd>

  <dt>Create time:</dt>
  <dd><%= @restaurant_photo.create_time %></dd>

  <dt>Modified time:</dt>
  <dd><%= @restaurant_photo.modified_time %></dd>

</dl>
