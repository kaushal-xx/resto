<div class="page-header">
  <%= link_to restaurant_musics_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_restaurant_music_path(@restaurant_music), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show restaurant_music</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @restaurant_music.name %></dd>

  <dt>Description:</dt>
  <dd><%= @restaurant_music.description %></dd>

  <dt>Restaurant:</dt>
  <dd><%= @restaurant_music.restaurant_id %></dd>

  <dt>Music type:</dt>
  <dd><%= @restaurant_music.music_type %></dd>

  <dt>Status:</dt>
  <dd><%= @restaurant_music.status %></dd>

  <dt>Creator:</dt>
  <dd><%= @restaurant_music.creator_id %></dd>

  <dt>Modifier:</dt>
  <dd><%= @restaurant_music.modifier_id %></dd>

  <dt>Create time:</dt>
  <dd><%= @restaurant_music.create_time %></dd>

  <dt>Modified time:</dt>
  <dd><%= @restaurant_music.modified_time %></dd>

</dl>
