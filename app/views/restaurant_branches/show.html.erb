<div class="page-header">
  <%= link_to restaurant_branches_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_restaurant_branch_path(@restaurant_branch), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show restaurant_branch</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @restaurant_branch.name %></dd>

  <dt>Description:</dt>
  <dd><%= @restaurant_branch.description %></dd>

  <dt>Brand logo url:</dt>
  <dd><%= @restaurant_branch.brand_logo_url %></dd>

  <dt>Phone:</dt>
  <dd><%= @restaurant_branch.phone %></dd>

  <dt>Email:</dt>
  <dd><%= @restaurant_branch.email %></dd>

  <dt>Restaurant:</dt>
  <dd><%= @restaurant_branch.restaurant_id %></dd>

  <dt>Location:</dt>
  <dd><%= @restaurant_branch.location_id %></dd>

  <dt>Status:</dt>
  <dd><%= @restaurant_branch.status %></dd>

  <dt>Creator:</dt>
  <dd><%= @restaurant_branch.creator_id %></dd>

  <dt>Modifier:</dt>
  <dd><%= @restaurant_branch.modifier_id %></dd>

  <dt>Create time:</dt>
  <dd><%= @restaurant_branch.create_time %></dd>

  <dt>Modified time:</dt>
  <dd><%= @restaurant_branch.modified_time %></dd>

</dl>
