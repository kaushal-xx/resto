<div class="page-header">
  <%= link_to user_orders_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_user_order_path(@user_order), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show user_order</h1>
</div>

<dl class="dl-horizontal">
  <dt>Order number:</dt>
  <dd><%= @user_order.order_number %></dd>

  <dt>Table booking:</dt>
  <dd><%= @user_order.table_booking_id %></dd>

  <dt>User:</dt>
  <dd><%= @user_order.user_id %></dd>

  <dt>Order amount:</dt>
  <dd><%= @user_order.order_amount %></dd>

  <dt>Order date:</dt>
  <dd><%= @user_order.order_date %></dd>

</dl>
